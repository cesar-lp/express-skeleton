{
  "name": "express-template",
  "version": "0.1.0",
  "description": "Express template.",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env APP_ENV=development && nodemon",
    "prod": "cross-env APP_ENV=production && npm run build && node build/src/main/index.js",
    "test": "cross-env APP_ENV=test && jest --testTimeout=1000",
    "coverage": "jest --coverage",
    "build": "rimraf ./build && tsc",
    "lint": "eslint --fix . --ext .ts"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/src/tests/.env-test.js"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/src/tests/**/**.test.[jt]s"
    ],
    "verbose": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesar-lp/express-template.git"
  },
  "keywords": [
    "express",
    "express-template"
  ],
  "author": "Cesar L. Palacios",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cesar-lp/express-template/issues"
  },
  "homepage": "https://github.com/cesar-lp/express-template#readme",
  "dependencies": {
    "@types/express": "^4.17.3",
    "@types/morgan": "^1.9.0",
    "@types/node": "^13.9.5",
    "@types/swagger-jsdoc": "^3.0.2",
    "@types/swagger-ui-express": "^4.1.1",
    "@types/uuid": "^7.0.3",
    "@types/validator": "^12.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-validator": "^6.4.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^4.0.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.2.7",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  }
}
